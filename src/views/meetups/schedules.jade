extends ../layout

block content
  table.table.table-striped.table-bordered.table-condensed
    thead
      tr
        th Round
        th User A
        th User 1
        th Score

    tbody
      for date in matches
        tr
          td #{date.round}
          td: a(href="/meetup/#{meetup.name}/schedule/#{date.user1}") #{date.user1}
          td: a(href="/meetup/#{meetup.name}/schedule/#{date.user2}") #{date.user2}
          td #{date.score}

  pre= matches


