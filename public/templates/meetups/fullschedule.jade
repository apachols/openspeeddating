.col-sm-10
  .panel.panel-default
    .panel-heading
      h1 Schedule
    .panel-body
      table.table.table-striped.table-condensed
        thead
          tr
            th Round
            th User A
            th User 1
            th Score

        tbody
          tr(ng-repeat="date in dates")
            td {{date.round}}
            td: a(href="#/meetups") {{date.user1.name}}
            td: a(href="#/meetups") {{date.user2.name}}
            td {{date.score}}