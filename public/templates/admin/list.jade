.panel.panel-default
  .panel-heading
    a.pull-right(ng-if="config.create" ng-click="add()") add
    h1 {{config.tableheader}}
  .panel-body
    table.table.table-striped
      thead
        tr
          th(ng-repeat="field in config.fields") {{field}}
          th.col-sm-2 options
      tbody
        tr(ng-repeat="doc in docs")
          td(ng-repeat="field in config.fields") {{doc[field]}}
          td: a(ng-click="update(doc)") edit
          td: a(ng-click="remove($index, doc._id)") remove
